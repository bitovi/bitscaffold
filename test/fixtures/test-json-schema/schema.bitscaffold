{
    "models": {
        "Employee": {
            "tableName": "employee",
            "fields": {
                "id": {
                    "type": "integer",
                    "primary": true,
                    "required": true,
                    "autoIncrement": true
                },
                "name": {
                    "type": "string",
                    "required": true
                },
                "start_date": {
                    "type": "date",
                    "required": false
                },
                "end_date": {
                    "type": "date",
                    "required": false
                }
            },
            "relationships": {
                "HasMany": {
                    "Skill": {
                        "from": "id",
                        "to": "id",
                        "through": "employee__skill"
                    }
                }
            },
            "validations": {
                "start_date": {
                    "lt": "end_date"
                },
                "end_date": {
                    "gt": "start_date"
                }
            }
        },
        "Skill": {
            "tableName": "skill",
            "fields": {
                "id": {
                    "type": "uuid",
                    "primary": true,
                    "required": true
                },
                "name": {
                    "type": "string",
                    "required": true,
                    "unique": true
                }
            },
            "relationships": {
                "HasMany": {
                    "Employee": {
                        "from": "id",
                        "to": "id",
                        "through": "employee__skill"
                    }
                }
            }
        }
    }
}