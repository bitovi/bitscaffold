datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-sequelize-generator"
}

model Employee {
  id         String   @id @default(uuid())
  name       String
  start_date DateTime
  end_date   DateTime
  createdAt  DateTime @default(now())
}

validate Employee {
  start_date  @lt("end_date")
  end_date    @gt("start_date")
}